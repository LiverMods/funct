    Title = "Loading";
    Text = "Disabling HitBox";
    Duration = 2;
})
wait(2)
game.StarterGui:SetCore("SendNotification", {
    Title = "Loading";
    Text = "Disabling HitBox";
    Duration = 2;
})
wait(2)
